<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#efefef"
    android:weightSum="1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#065299"
        android:orientation="horizontal"
        android:weightSum="1">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            app:srcCompat="@drawable/image_logo"
            android:contentDescription="@string/todo" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="105dp"
            android:layout_marginTop="10dp"
            android:text="@string/설정"
            android:textSize="22sp"
            android:textStyle="bold"
            tools:textColor="#e0ffffff" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:layout_marginTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="알림 설정"
            android:layout_marginLeft="5dp"
            android:textSize="15dp"
            android:textColor="#b1b1b1"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="44dp"

            android:layout_marginLeft="5dp"
            android:text="모든 알림"
            android:textColor="#666666"
            android:textSize="25sp" />

        <Switch
            android:id="@+id/SW_AllSetting"
            android:layout_width="0dp"
            android:layout_height="44dp"
            android:layout_weight="1"
            android:layout_marginEnd="10dp"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:background="#ffffff">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:text="소리"
            android:textColor="#666666"
            android:textSize="25sp" />

        <Switch
            android:id="@+id/SW_Sound"
            android:layout_width="0dp"
            android:layout_height="44dp"
            android:layout_weight="1"
            android:layout_marginEnd="10dp"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:background="#ffffff">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:text="진동"
            android:textColor="#666666"
            android:textSize="25sp" />

        <Switch
            android:id="@+id/SW_Vibration"
            android:layout_width="0dp"
            android:layout_height="44dp"
            android:layout_weight="1"
            android:layout_marginEnd="10dp"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:background="#ffffff">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:text="팝업 메시지"
            android:textColor="#666666"
            android:textSize="25sp" />

        <Switch
            android:id="@+id/SW_Message"
            android:layout_width="0dp"
            android:layout_height="44dp"
            android:layout_weight="1"
            android:layout_marginEnd="10dp"
            />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:layout_marginTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="계정 정보"
            android:layout_marginLeft="5dp"
            android:textSize="15dp"
            android:textColor="#b1b1b1"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff">

        <TextView
            android:layout_width="wrap_content"
            android:id="@+id/Txt_email"
            android:layout_height="44dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:text="UserEmail@test.com"
            android:textColor="#666666"
            android:textSize="25sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:layout_marginTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="개발자 문의"
            android:layout_marginLeft="5dp"
            android:textSize="15dp"
            android:textColor="#b1b1b1"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:text="devEmail@gachon.ac.kr"
            android:textColor="#666666"
            android:textSize="25sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:layout_marginTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="버전 정보"
            android:layout_marginLeft="5dp"
            android:textSize="15dp"
            android:textColor="#b1b1b1"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff">

        <TextView
            android:layout_width="wrap_content"
            android:id="@+id/Txt_Version"
            android:layout_height="44dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:text="v 0.0.0"
            android:textColor="#666666"
            android:textSize="25sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:layout_marginTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="데이터 관리"
            android:layout_marginLeft="5dp"
            android:textSize="15dp"
            android:textColor="#b1b1b1"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff">

       <Button
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           android:text="사용자 정보 초기화"
           android:background="#e7e7e7"
           android:textSize="15dp"
           android:textColor="#b1b1b1"
           />
    </LinearLayout>




</LinearLayout>
